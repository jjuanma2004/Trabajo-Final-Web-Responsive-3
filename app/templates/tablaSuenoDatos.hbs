<table class="bordered responsive-table">
	<thead class="black-text">
		<tr>
			<th></th>
			<th>Fecha</th>
			<th>Tipo Sueño</th>
			<th>Hora Inicial</th>
			<th>Hora Final</th>
			<th>Recuperación</th>
		</tr>
	</thead>
	<tbody>
		{{#each suenos}}
		<tr>
			<td>
				<input type="checkbox" id="productSelection{{@index}}" class="filled-in primaryCheckbox tableCheckbox">
				<label for="productSelection{{@index}}"></label>
			</td>
			<td>{{fecha}}</td>
			<td>{{tipo}}</td>
			<td>{{horaIni}}</td>
			<td>{{horaFin}}</td>			
			<td>{{recuperacion}}</td>			
		</tr>
		{{/each}}
	</tbody>
</table>